priority 1

snippet #ifndef "#ifndef ... #define ... #endif"
#ifndef ${1/([A-Za-z0-9_]+).*/$1/}
#define ${1:SYMBOL}
#endif
endsnippet

snippet class "class { ... }"
class $1
{
	public: // CONSTRUCTORS
		$1(void) = default;
	public: // DESTRUCTOR
		~$1(void) = default;

	public: // VARIABLES

	public: // GETTERS
	public: // SETTERS

	public: // METHODS

	public: // OPERATORS

	private:
};
endsnippet
